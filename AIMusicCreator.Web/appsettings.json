{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Blazor": {
    "JSInteropDefaultCallTimeout": "00:02:00",
    "CircuitOptions": {
      "DisconnectedCircuitRetentionPeriod": "00:05:00"
    }
  },
  "Kestrel": {
    "Limits": {
      "KeepAliveTimeout": "00:05:00",
      "RequestHeadersTimeout": "00:02:00"
    }
  }
}
